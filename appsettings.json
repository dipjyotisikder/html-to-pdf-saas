{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ApiKeySettings": {
    "HeaderName": "X-API-Key",
    "ValidApiKeys": [
      "dev-key-12345-replace-in-production",
      "test-key-67890-replace-in-production"
    ]
  },
  "RequestLimits": {
    "MaxHtmlSizeBytes": 2097152,
    "MaxGenerationTimeoutSeconds": 30,
    "MaxConcurrentJobsPerKey": 5
  },
  "IpRateLimiting": {
    "EnableEndpointRateLimiting": true,
    "StackBlockedRequests": false,
    "RealIpHeader": "X-Real-IP",
    "ClientIdHeader": "X-ClientId",
    "HttpStatusCode": 429,
    "GeneralRules": [
      {
        "Endpoint": "*/pdf/*",
        "Period": "1m",
        "Limit": 10
      },
      {
        "Endpoint": "*/pdf/*",
        "Period": "1h",
        "Limit": 100
      }
    ]
  }
}
